datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id         Int      @id @default(autoincrement())
  name       String?
  email      String   @unique
  image      String?
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}

model Rating {
  id            Int    @id @default(autoincrement())
  name          String
  email         String
  customerNumber String
  rating        Int
  createdAt  DateTime @default(now())
}
